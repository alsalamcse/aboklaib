<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aboklaib.moaadtaskmanger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainAllTasksActivity"
            android:label="@string/title_activity_main_all_tasks"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".wellcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signin" />
        <activity android:name=".signup" />
        <activity android:name=".alltask" />
        <activity android:name=".setting" />
        <activity android:name=".addtask" />
        <activity android:name=".TempAllTaskActivity" />
    </application>

</manifest>